@if (ViewBag.Data != null && ViewBag.Data.Count > 0)
{
    <div class="row-fluid">
        <div class="span12">
            <legend>Zadnji rezultati:</legend>    
        </div>
    </div>

    for (int i = 0; i < ViewBag.Data.Count; i++)
    {
        if (!string.IsNullOrEmpty(ViewBag.Data[i].URLFile))
        {
            <div class="row-fluid">
                <div class="span12">
                    @if (!string.IsNullOrEmpty(ViewBag.Data[i].FileName))
                    {
                        <a href="@Url.Content(@ViewBag.Data[i].URLFile)"><i class="icon-chevron-right" style="margin-top: 1px"></i> @Url.Content(@ViewBag.Data[i].FileName)</a>
                    }
                    else
                    {
                        <a href="@Url.Content(@ViewBag.Data[i].URLFile)"><i class="icon-chevron-right" style="margin-top: 1px"></i> @Url.Content(@ViewBag.Data[i].URLFile)</a>
                    }
                </div>
            </div>
        }
    }
    <div class="row-fluid">
        <div class="span12">
        </div>
    </div>
}